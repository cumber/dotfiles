:set editor vim
:set prompt "\ESC[33m%s\n\ESC[32;1m\STXÎ»\ESC[m\STX "
:set prompt2 "\ESC[32;1m\STX|\ESC[m\STX "
:set +m +t
:def hoogle \x -> return $ ":! hoogle search -cl \"" ++ x ++ "\""
:def doc \x -> return $ ":! hoogle search -cl --info \"" ++ x ++ "\""
:seti -fdefer-typed-holes
:seti -fdefer-type-errors
:seti -fno-warn-type-defaults
:seti -XOverloadedStrings
:seti -XPartialTypeSignatures
:seti -fno-warn-partial-type-signatures
